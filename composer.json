{
    "name": "ccasanovas/cakeapigateway",
    "description": "ApiGatewaySDK plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ccasanovas\\ApiGatewaySDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ccasanovas\\ApiGatewaySDK\\Test\\": "tests",
            "Ccasanovas\\ApiGatewaySDK\\Test\\Fixture\\": "tests"
        }
    }
}
